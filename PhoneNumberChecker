function telephoneCheck(str) {
  if (/^1?\s?\d{3}-?\s?\d{3}-?\s?\d{4}$/.test(str)) {
    console.log(true);
    return true;
  } else {
  console.log(false)}
  return false;
}

telephoneCheck('1 123 456 7890');

